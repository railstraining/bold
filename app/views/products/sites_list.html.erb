<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Party Id</th>
      <th>Country</th>
      <th>State</th>
      <th>User Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sites.each do |site| %>
      <tr>
        <td><%= site.name %></td>
        <td><%= site.party_id %></td>
        <td><%= site.country %></td>
        <td><%= site.state %></td>
        <td><%= site.user_name %></td>
        <td><%= link_to 'Show', site %></td>
        <td><%= link_to 'Edit Site', edit_site_path(site) %></td>
        <td><%= link_to 'Destroy', site, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>


